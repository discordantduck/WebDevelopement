{% extends "clubBase.njk" %}

{% block metaStuff %}
    <title>Users</title>
{% endblock %}
    
{% block main %}
    <main>
        <table>
            <thead>
                <tr>
                    <th>First Name</th>
                    <th>Last Name</th>
                    <th>Email</th>
                    <th>Role</th>
                </tr>
            </thead>
            {%for user in users %}
                <tbody>
                    <th>{{user.firstName}}</th>
                    <th>{{user.lastName}}</th>
                    <th>{{user.email}}</th>
                    <th>{{user.role}}</th>
                </tbody>
            {% endfor %}
        </table>
    </main>
{% endblock %}